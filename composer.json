{
    "name": "jzaaa/cake-captcha",
    "description": "CakeCaptcha plugin for CakePHP",
    "keywords": ["CakePHP4 Captcha", "CakePHP", "Captcha"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.4",
        "s1syphos/php-simple-captcha": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "JZaaa\\CakeCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JZaaa\\CakeCaptcha\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
